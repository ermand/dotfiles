set nocompatible              " be iMproved, required

"so ~/.vim/plugins.vim

syntax enable


set backspace=indent,eol,start
let mapleader = ','
set number
set linespace=13

"------------------Visuals------------------"
"colorscheme atom-dark-256
set t_CO=256
set guifont=Fira_Code:h14


"------------------Search------------------"
set hlsearch
set incsearch
